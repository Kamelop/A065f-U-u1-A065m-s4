config MTK_FINGERPRINT_O8_SUPPORT
    tristate "O8 Fingerprint support"
    help
        Fingerprint mtk driver enable/disable
        in the kernel.Say Y here if you want to
        use mediatek fingerprint driver,fingerprint
        driver will support fingerprint function

config MTK_FINGERPRINT_SELECT
    string "sensor type"
    default "GF318M"

source "$(KCONFIG_EXT_PREFIX)drivers/misc/mediatek/fingerprint/fingerprint_O8/silead/Kconfig"
source "$(KCONFIG_EXT_PREFIX)drivers/misc/mediatek/fingerprint/fingerprint_O8/jiiov/Kconfig"
source "$(KCONFIG_EXT_PREFIX)drivers/misc/mediatek/fingerprint/fingerprint_O8/focaltech/Kconfig"


